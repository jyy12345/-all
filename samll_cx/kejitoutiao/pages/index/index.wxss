/*menubar*/
page{
  position: relative
}
.laeb{position: fixed; height: 90rpx;
    font-size: 30rpx;
    font-weight: 600;line-height: 90rpx;background: white;width: 88rpx;text-align: center; color:#666666;}
.menubar{
    height: 90rpx;
    background: #ffffff;
    position: fixed;
    left:0;
    top:0;
    z-index: 99;
    white-space: nowrap;
    display: flex;
}
.menu{
    /* width:700rpx; */
    height:90rpx;
    line-height: 90rpx;
    float:left;
    background: #fff;
    margin-left: 90rpx;
}
.menu2{
  position: fixed;
  top:0;
  right:0rpx;
  width: 150rpx;
  height: 90rpx;
  z-index: 199;
  background: #fff;
  box-shadow: 0px 0px 3px #eecccc;
}
.menu2 .menu2Img1{
  width: 50%;
  height: 90rpx;
  display:flex;
  float: left;      
  justify-content: center;  
  align-items:center; 
}
.menu2 .menu2Img1 image{
  width: 35rpx;
  height: 37rpx;
}
.menu view{
    float:left;
    height: 90rpx;
    /* padding:0 25rpx; */
    /* width: 88rpx; */
    /* text-align: center; */
    padding:0 10rpx;
}
.menu view text{
    display: block;
    font-size: 30rpx;
    font-weight: 600;
    height: 86rpx;
    text-align: center;
    color:#666666;
}
.laeb text{
  height: 86rpx;
  display: block;
}
.laeb text.active{
    color:#0081d2;
    border-bottom: 6rpx solid #0081d2;
}
.menu  text.active{
    color:#0081d2;
    border-bottom: 6rpx solid #0081d2;
}
.swiper{
  position: relative
}
.wrap{
  width: 100%;
  height: auto;
  position: relative;
}
.wrap .banerbot{
  display: block;
  position: absolute;
  bottom:0;
  width: 100%;
  height: 253rpx;
}
.wrap text{
  display: block;
  position: absolute;
  width: 96%;
  left:2%;
  right:0;
  bottom:45rpx;
  margin:0 auto;
  color: #fff;
  font-size: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  text-align: center;
}
.swiper{
  width: 100%;
  height: 350rpx;
}
.swiper image{
  width: 100%;
  height: 350rpx;
}
.content{
  margin-top:90rpx;
}
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.swiper .wx-swiper-dot:first-child{
  margin-left: 660rpx;
}
.swiper .wx-swiper-dot{
    width:6rpx;
    height: 12rpx;
    border-radius: 4rpx; 
    margin-left:-4rpx;
}
.swiper .wx-swiper-dot-active{
    height: 18rpx;
}
.information{
  width:100%;
  height: 260rpx;
  border-bottom: 1px solid #f0f0f0;
  overflow: hidden;
}
.infCen{
  width: 94%;
  margin:35rpx 3%;
  height:190rpx;
}
.infCenLeft{
  width: 35%;
  height: 100%;
  float: left;
}
.infCenLeft image{
  width: 100%;
  height: 100%;
}
.infCenRight{
  float: left;
  width: 63%;
  height: 100%;
  margin-left: 2%;
}
.infCenRight1{width: 100%;height: 100%;
  margin-left: 2%;}
.infCenRightTop{
  width: 100%;
  height: 60%;
  color: #222222;
  font-size: 35rpx;
  line-height: 60rpx;
  margin-top: -10rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
}
.infCenRightBot{
  width: 100%;
  height: 20%;
  margin-top: 53rpx;
  font-size: 30rpx;
}
.infCenRightBot text:nth-child(1){
  color: #0081d2;
}
.infCenRightBot text:nth-child(2){
  margin-left:20rpx;
  color: #a6a6a6;
}
.zhezhao,.zhezhaoSou{
  width:100%;
  height:774px;
  background:#fff;
  position: fixed;
  left:0;
  top:0;
  z-index: 299;
  overflow: hidden;
}
.zhezhaoSou{padding-top: 10rpx;}
.zhezhaoSou input{display: inline-block;width: 65%;height: 70rpx;border: 1px solid #f5f5f5;margin-left: 25rpx;padding-left: 10rpx;font-size: 25rpx;color: #a6a6a6;margin-top: 30rpx;}
#list{
  width: 94%;margin: 0 auto;
}
#list view{
  width:100%;
  height:60rpx;
  line-height: 60rpx;
  border-bottom: 1px solid #f0f0f0;
  font-size: 25rpx;
}
.inpFont{font-size: 25rpx;}
.zhezhaoSou button{width: 20%;display: inline-block;height: 70rpx;line-height: 70rpx;font-size: 26rpx;background: #0081d2;color: white;margin-right: 25rpx;margin-top:30rpx;
float: right;}
.zhezhaoTop{
  width:90%;
  height: 50rpx;
  margin:25rpx 0 0 25rpx;
}
.zhezhaoTop image{
  width: 50rpx;
  height: 50rpx;
}
.zhezhaoCen{
  width: 100%;
  height: 60rpx;
  margin-top: 18rpx;
}
.zhezhaoCen text:nth-child(1){
  font-size: 35rpx;
  display:block;
  float: left;
  margin-left:20rpx;
}
.zhezhaoCen text:nth-child(2){
  font-size: 25rpx;
  color:rgb(133, 133, 133);
  display:block;
  float: left;
  margin-left:20rpx;
  line-height: 60rpx;
}
.zhezhaoBot{
  width: 100%;
  padding-bottom:30rpx;
  display: flex;
  flex-wrap:  wrap ;
  align-content: flex-start ;
}
.lanmu{
  width:150rpx;
  height: 60rpx;
  background: #f5f5f5;
  border-radius: 8rpx;
  margin:20rpx 0 0 30rpx;
  color: rgb(39, 39, 39);
  position: relative;
}
.lanmu text{
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  font-size: 30rpx;
  text-align: center;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.lanmu image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right:-10rpx;
  top:-10rpx;
}