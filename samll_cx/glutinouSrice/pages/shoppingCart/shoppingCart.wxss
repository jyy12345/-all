   .stepper {
	/* width: 80px; */
	height: 26px;
	/*给主容器设一个边框*/
	/* border: 1px solid #ccc; */
	border-radius: 3px;
	margin:0 auto;
}
 
/*加号和减号*/
.stepper text {
	width: 43rpx;
  height: 43rpx;
	line-height:43rpx;
	text-align: center;
	float: left;
  background: #f5f5f5;
  border-radius: 50%;
}
 
/*数值*/
.stepper input {
	width: 35px;
	height: 43rpx;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 12px;
	/*给中间的input设置左右边框即可*/
	/* border-left: 1px solid #ccc; */
	/* border-right: 1px solid #ccc; */
}
 
/*普通样式*/
.stepper .normal{
	color: black;
}
 
/*禁用样式*/
.stepper .disabled{
	color: #ccc;
}
