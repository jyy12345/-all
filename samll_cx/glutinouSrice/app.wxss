/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 
.auto{width: 94%;margin: 0 auto;}
page{  background: #f4f4f4}
/* 清除缓存 */
.clearFlo::after{display: block;height: 0;clear: both;content: ''}
/* 浮动 */
.flR{float: right;}
.flL{float: left;}
/*定义滚动条轨道 内阴影+圆角*/ 
::-webkit-scrollbar{ 
  width: 2rpx;
-webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.514); 
border-radius: 10px; 
opacity: 0;
} 
.pd_inp view{font-size: 24rpx;}
.regi_btn {
width: 90%;
margin: 30rpx auto;
background: red;
color: white;
text-align: center;
padding: 28rpx 0!important;
border-radius: 60rpx;
font-size: 30rpx!important;

}
.twoLin{    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;}
.oneLin{display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;}
 .book_title {height: 60rpx;}
 .book_price {margin-left: 15rpx;}
 .socus {
  margin-top: 5rpx;
}
.book_del {text-decoration: line-through;
}

