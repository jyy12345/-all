/*mask*/  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.3;  
  overflow: hidden;  
}  
  
/*content*/  
.drawer_box {  
  width: 510rpx;  
  overflow: hidden;  
  position: fixed;  
  top: 45%;  
  left: 10%;  
  z-index: 1001;  
  background: #F1FDF1; 
  margin: -150px 50rpx 0 50rpx;  
  border-radius: 13px;  
}  
  
.drawer_title{  

  width: 455rpx;
  height: 165rpx;
  margin: 30rpx auto;
   background: url("data:image/png;base64,");background-size: 100% 100%;
}  
.drawer_content {  
  height: 220px;  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
}  
  
.btn_ok{  
  width: 190rpx;
  height: 70rpx;
