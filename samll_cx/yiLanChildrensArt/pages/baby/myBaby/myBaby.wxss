/* 弹框 */
/*mask*/  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.3;  
  overflow: hidden;  
}  
  
/*content*/  
.drawer_box {  
  width: 510rpx;  
  overflow: hidden;  
  position: fixed;  
  top: 35%;  
  left: 10%;  
  z-index: 1001;  
  background: #EDE2F8;  
  margin: -150px 50rpx 0 50rpx;  
  border-radius: 13px;  
}  
  
.drawer_title{  

  width: 400rpx;
  height: 120rpx;
  margin: 30rpx auto;
   /* background: url();background-size: 100% 100%; */
}  
.drawer_content {  
  height: 300px;  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
}  
  
