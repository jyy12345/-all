/* 轮播 */
.page-section{width: 94%;margin: 0 auto;}
swiper-item image{border-radius: 30rpx; width: 100%;height: 100%;}
.swiper .wx-swiper-dot:first-child{
  margin-left: 610rpx;
}
.swiper .wx-swiper-dot{
    width:9rpx;
    height: 15rpx;
    border-radius: 4rpx; 
    margin-left:-4rpx;
}
.swiper .wx-swiper-dot-active{
    height: 21rpx;
}
.banner{ 
  position: relative;
}
.banner text{ 
  position: absolute;
    bottom:0;
    left:0;
    /* transform:translateX(-50%); */
    color: #fff;
    width: 100%;
    height: 60px;
    line-height: 160rpx;
    background: linear-gradient(to top,rgba(10, 10, 10, 0.7),
   rgba(207, 205, 216, 0.1),rgba(170, 169, 173, 0.3));
    border-bottom-right-radius: 30rpx;
    border-bottom-left-radius: 30rpx;
    font-size: 14px;
    text-indent:40rpx;
}
