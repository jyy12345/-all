@import "/pages/tabbar/tabbar.wxss";
page{background-color: #b0daec;}
.title1{color: white;font-family: 'Tensentype-XiChaoYuanJ';font-weight: 600;font-size: 45rpx;text-align: center;margin-top: 6.1%;}
textarea{margin: 0 auto;margin-top: 32%;width: 65%;}
.textAre{font-size:29rpx;font-family: 'PingFangSC-Medium';color: #fff;}
.buton{width: 280rpx;height: 100rpx;background: url("data:image/png;base64,");background-size: 100% 100%;margin: 10% auto;}
  
/*mask*/  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.3;  
  overflow: hidden;  
}  
  
/*content*/  
.drawer_box {  
  width: 510rpx;  
  overflow: hidden;  
  position: fixed;  
  top: 45%;  
  left: 10%;  
  z-index: 1001;  
  background: #dfe8ec;  
  margin: -150px 50rpx 0 50rpx;  
  border-radius: 13px;  
}  
  
.drawer_title{  

  width: 400rpx;
  height: 120rpx;
  margin: 30rpx auto;
   background: url("data:image/png;base64,");background-size: 100% 100%;
}  
.drawer_content {  
  height: 280px;  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
}  
  
.btn_ok{  
  width: 190rpx;
  height: 70rpx;
  border-radius: 20rpx;
  background-color: #8dd5f4;
  font: 20px "microsoft yahei";  
  text-align: center;  
  line-height: 70rpx;
  font-size: 25rpx;
  border-top: 1px solid #E8E8EA;  
  color:#fff;  
  margin: 0 auto;
  margin-top: 70rpx;
}  
.grid{width:400rpx;margin: 0 auto;}
.input_base{width:100%;height: 80rpx; background-color:#fff;border-radius: 15rpx;margin-top: 20rpx;font-size: 26rpx; padding-left: 20rpx;}
.redParty{color: #9edbf5;}
.borderBottom{width: 100%;height: 19rpx;background:url("data:image/png;base64,");background-size: 100% 100%; margin-top: 80rpx;}
.contact{font-size: 24rpx;color: #8DD5F4;margin: 20rpx 0 0 30rpx;}
.imgPostBox{position: fixed;right: 50rpx;top: 80rpx;z-index:9999;
}
.imgPost{width: 76rpx;height: 76rpx;z-index:9999;}
/* 浮标 */
.bgColor{width: 124rpx;height: 130rpx;position: absolute;left: 100%;top:35%;z-index: 9999;}
.bgColor image{width: 100%;height: 100%;}
  
/*定义滚动条轨道 内阴影+圆角*/ 
::-webkit-scrollbar-track { 
-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
border-radius: 10px; 
opacity: 0;
} 
