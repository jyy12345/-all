.brand {
  min-height: 100vh;
  padding: 20rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.branduls {
  display: flex;
  flex-wrap: wrap;
}
.brandlis {
  width: 229rpx;
  height: 173rpx;
  border-radius: 15rpx;
  position: relative;
  background: red;
  margin-bottom: 15rpx;
  margin-right: 10rpx;
}
.brandlis:nth-child(3n) {
  margin-right: 0;
}
.brandlis img {
  width: 100%;
  height: 100%;
}